    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.80.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.00",
    "vite": "^5.4.9"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "npm run format"
  }
